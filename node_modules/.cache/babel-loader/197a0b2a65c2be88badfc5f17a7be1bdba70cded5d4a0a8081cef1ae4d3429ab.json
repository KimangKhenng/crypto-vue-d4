/span>\n    <button @click=\"deleteItem(index)\" class=\"button\">Delete</button>\n    <button v-if=\"!item.completed\" @click=\"completeItem(index)\" class=\"button\">\n      Complete\n    </button>\n  </div>\n\n  <h3 v-if=\"completed.length > 0\">Completed Tasks</h3>\n  <div class=\"task\" v-for=\"(item, index) in completed\" :key=\"index\">\n    <span>\n      <s>{{ index + 1 }} - {{ item }}</s></span\n    >\n    <button @click=\"deleteCompletedItem(index)\" class=\"button\">Delete</button>\n  </div>\n\n  <h3>All Tasks</h3>\n  <div class=\"task\" v-for=\"(item, index) in getAllArray\" :key=\"index\">\n    <span> {{ index + 1 }} - {{ item }}</span>\n  </div>\n</template>\n<script>\nexport default {\n  data() {\n    return {\n      todos: [],\n      task: \"\",\n      completed: [],\n      hoverIndex: 0,\n    };\n  },\n  methods: {\n    updateLocalStorage() {\n      localStorage.setItem(\"todos\", JSON.stringify(this.todos));\n      localStorage.setItem(\"todosCompleted\", JSON.stringify(this.completed));\n    },\n    addTask() {\n      this.todos.push(this.task);\n      this.task = \"\";\n      this.updateLocalStorage();\n    },\n    deleteItem(index) {\n      this.todos.splice(index, 1);\n      this.updateLocalStorage();\n    },\n    deleteCompletedItem(index) {\n      this.completed.splice(index, 1);\n      this.updateLocalStorage();\n    },\n    completeItem(index) {\n      this.completed.push(this.todos[index]);\n      this.deleteItem(index);\n      this.updateLocalStorage();\n    },\n    handleDoubleClick() {\n      console.log(\"Hello Double Click\");\n    },\n    handleMouseOver(index) {\n      this.hoverIndex = index;\n    },\n  },\n};\n</script>"],"mappings":";AAqCA,eAAe;EACbA,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,IAAI,EAAE,EAAE;MACRC,SAAS,EAAE,EAAE;MACbC,UAAU,EAAE;IACd,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,kBAAkBA,CAAA,EAAG;MACnBC,YAAY,CAACC,OAAO,CAAC,OAAO,EAAEC,IAAI,CAACC,SAAS,CAAC,IAAI,CAACT,KAAK,CAAC,CAAC;MACzDM,YAAY,CAACC,OAAO,CAAC,gBAAg